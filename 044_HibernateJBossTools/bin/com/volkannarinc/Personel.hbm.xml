<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
                                   "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

	<class name="com.volkannarinc.Personel" table="PERSONEL">
		<meta attribute="class-description">PERSONEL SINIFININ DETAYLARI</meta>
		
		<id column="PERSONEL_ID" name="personelId" type="long">
			<generator class="native"></generator>
		</id>
		
		<property name="personelAdi" column="PERSONEL_ADI" generated="never" 
				lazy="false" not-null="true" type="string"></property>
	</class>

</hibernate-mapping>
